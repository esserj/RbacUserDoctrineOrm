    "type": "library",
    "keywords": [
        "rbac",
        "user",
        "doctrine",
        "orm",
        "module",
        "zf2"
    ],
    "homepage": "http://www.github.com/esserj/RbacUserDoctrineOrm/",
    "authors": [
        {
            "name": "Jan Esser",
            "email": "esserj@gmail.com",
            "homepage": ""
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": ">2.2.0rc1",
        "zf-commons/zfc-user": "dev-master",
        "zf-commons/zfc-rbac": ">=0.2.2",
        "zf-commons/zfc-user-doctrine-orm": "dev-master",
        "doctrine/doctrine-orm-module": "0.7.*"
    },

    "autoload": {
        "psr-0": {
            "RbacUserDoctrineOrm": "src/"
        }
    }
}
